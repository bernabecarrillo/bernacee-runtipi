{
  "services": [
    {
      "name": "actual_server",
      "image": "docker.io/actualbudget/actual-server:latest",
      "internalPort": 5006,
      "isMain": true,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/actual-data",
          "containerPath": "/data",
          "readOnly": false,
          "shared": false,
          "private": false
        }
      ],
      "healthCheck": {
        "test": "node src/scripts/health-check.js",
        "interval": "60s",
        "timeout": "10s",
        "retries": 3
      }
    }
  ]
}